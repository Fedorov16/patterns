{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "ext-pdo": "*",
        "ext-simplexml": "*",
        "doctrine/annotations": "1.*",
        "doctrine/cache": "1.*",
        "guzzlehttp/guzzle": "^7.0",
        "guzzlehttp/psr7": "^2.2",
        "spatie/array-to-xml": "^3.1",
        "symfony/config": "5.*",
        "symfony/framework-bundle": "5.*",
        "symfony/routing": "5.*",
        "symfony/yaml": "^6.0",
        "twig/twig": "^3.3"
    }
    ,
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-latest",
        "symfony/test-pack": "^1.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    }
}
