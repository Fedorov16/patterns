{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0.6",
        "doctrine/annotations": "1.*",
        "doctrine/cache": "1.*",
        "symfony/config": "5.*",
        "symfony/framework-bundle": "5.*",
        "symfony/routing": "5.*",
        "symfony/yaml": "^6.0",
        "twig/twig": "^3.3"
    }
    ,
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "App"
        }
    }
}
